{
  "name": "ellingham-technologies/phphelpers",
  "type": "library",
  "license": "MIT",
  "description": "A general library for PHP containing some common functionality with easy extensibility",
  "authors": [
	{
	  "name": "Ellingham Technologies Ltd",
	  "email": "dev@ellinghamtech.co.uk"
	}
  ],
  "require": {
	"php": ">=7.1.0",
	"ext-pdo": "*",
	"ext-curl": "*",
	"ext-json": "*"
  },
  "suggest": {
	"ext-sqlite" : "For using SQLite in the Database Wrappers",
	"ext-mysql" : "For using MySQL/MariaDB in the Database Wrappers"
  },
  "autoload": {
	"psr-4": {
	  "EllinghamTech\\Database\\": "src/EllinghamTech/Database/",
	  "EllinghamTech\\PaginationHelpers\\": "src/EllinghamTech/PaginationHelpers/",
	  "EllinghamTech\\Recaptcha\\": "src/EllinghamTech/Recaptcha/",
	  "EllinghamTech\\Session\\": "src/EllinghamTech/Session/",
	  "EllinghamTech\\Templating\\": "src/EllinghamTech/Templating/"
	}
  },
  "minimum-stability": "dev"
}